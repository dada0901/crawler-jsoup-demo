public class BodyPart {

	private int id;
	private String name;
	private String hotIllnessRef;
	private String illnessRef;
	private String hotSymptomRef;
	private String symptomRef;

	public String getHotIllnessRef() {
		return hotIllnessRef;
	}

	public void setHotIllnessRef(String hotIllnessRef) {
		this.hotIllnessRef = hotIllnessRef;
	}

	public String getHotSymptomRef() {
		return hotSymptomRef;
	}

	public void setHotSymptomRef(String hotSymptomRef) {
		this.hotSymptomRef = hotSymptomRef;
	}

	public String getIllnessRef() {
		return illnessRef;
	}

	public void setIllnessRef(String illnessRef) {
		this.illnessRef = illnessRef;
	}

	public String getSymptomRef() {
		return symptomRef;
	}

	public void setSymptomRef(String symptomRef) {
		this.symptomRef = symptomRef;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	@Override
	public String toString(){
		return this.name;
	}
}
